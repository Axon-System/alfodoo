<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="CmisDocumentReconcileView">
        <div class="o_form_view"><div class="oe_form_sheetbg o_form_sheet_bg"><div class="oe_form_sheet oe_form_sheet_width o_form_sheet">
            <h1><t t-esc="title"/></h1>
            <div class="progress progress-striped">
                <div class="progress-bar" role="progressbar" aria-valuenow="0" aria-valuemin="0" t-att-aria-valuemax="total_lines" style="width: 0%;">
                    <div class="progress-text"><span class="valuenow">0</span> / <span class="valuemax"><t t-esc="total_lines"/></span></div>
                </div>
            </div>
            <div class="oe_clear o_clear"></div>
            <div class="reconciliation_form">
                <div class="btn-toolbar">
                  <div class="btn-group">
                    <button type="button" class="oe_button oe_form_button_create"><t t-esc="label_button_create"/></button>
                  </div>
                  <div class="btn-group">
                    <button type="button" class="oe_button oe_form_button_link"><t t-esc="label_button_link"/></button>
                  </div>
                  <div class="oe_right">
                      <div class="oe_list_pager"/>
                  </div>
                </div>
                <div class="oe_clear o_clear"></div>
                <div class="documentpreview panel"></div>
                <!-- p class='protip'>Tip : Hit ctrl-enter to reconcile all balanced items.</p -->
            </div>
        </div></div></div>
    </t>
</templates>